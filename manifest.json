        "change_urgency": "medium"
    },
    "architecture": {
        "rocky": "x86_64",
        "debian": "amd64",
        "ubuntu": "amd64"
    },
    "dependencies": {
        "ubuntu_common": [
            "cockpit",
            "python3",
            "rsync",
            "zip",
            "file",
            "coreutils"
        ],
        "rocky_common": [
            "cockpit",
            "python3",
            "rsync",
            "zip",
            "file"
        ]
    },
    "builds": [
        {
            "group": "ubuntu",
            "os_name": "ubuntu-focal",
            "image": "ci.cr.45d.io/ci/simple/ubuntu-focal:1"
        },
        {
            "group": "ubuntu",
            "os_name": "ubuntu-jammy",
            "image": "ci.cr.45d.io/ci/simple/ubuntu-jammy:1"
        },
        {
            "group": "rocky",
            "os_name": "rocky-el8",
            "image": "ci.cr.45d.io/ci/simple/rocky-el8:1"
        },
        {
            "group": "rocky",
            "os_name": "rocky-el9",
            "image": "ci.cr.45d.io/ci/simple/rocky-el9:1"
        },
        {
            "group": "debian",
            "os_name": "debian-bookworm",
            "image": "ci.cr.45d.io/ci/simple/ubuntu-jammy:1"
        }
    ],
    "repos": [
        "community",
        "enterprise"
    ],
    "changelog": {
        "urgency": "medium",
        "version": "0.5.10",
